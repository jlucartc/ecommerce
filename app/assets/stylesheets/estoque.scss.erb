								box-shadow: 1px 1px 2px 0px #999999;
								padding: 5px 10px 5px 10px;

								&:hover{
									background-color: #0033ff;
								}

							}
							
							.produto-estoque-excluir{
								border: none;
								font-size: 1.1em;
								border-radius: 5px;
								background-color: #ff5522;
								color: #f0f0f0;
								box-shadow: 1px 1px 2px 0px #999999;
								padding: 5px 10px 5px 10px;

								&:hover{
									background-color: #ff3300;
								}

							}

							.produto-estoque-cancelar{
								border: none;
								border-radius: 5px;
								background-color: #ff5522;
								color: #f0f0f0;
								box-shadow: 1px 1px 2px 0px #999999;
								padding: 5px 10px 5px 10px;

								&:hover{
									background-color: #ff3300;
								}

							}

							.produto-estoque-salvar{
								border: none;
								border-radius: 5px;
								background-color: #11bb33;
								color: #f0f0f0;
								box-shadow: 1px 1px 2px 0px #999999;
								padding: 5px 10px 5px 10px;

								&:hover{
									background-color: #119933;
								}
							}

						}

					}
					
					.produto-adiciona-estoque{
						grid-column: 2 / 8;
						grid-row: 2;
						margin: 10px 0px 0px 0px;
						display: flex;
						flex-direction: column;
						align-items: start;

						.produto-adiciona-estoque-grupo{
							border-radius: 5px;
							background-color: #fdfdfd;
							display: flex;
							flex-direction: row;
							align-items: center;
							justify-content: center;
							box-shadow: 1px 1px 2px 0px #999999;

							.produto-adiciona-estoque-menos{
								border-top-left-radius: 5px;
								border-bottom-left-radius: 5px;
								background-color: #fdfdfd;
								border: none;
								height: 28px;
								box-sizing: border-box;
								padding: 0px 10px 0px 10px;

								&:hover{
									background-color: #999999;
									color: #ffffff;
								}
							}

							.produto-adiciona-estoque-quantidade{
								background-color: #fdfdfd;
								border: none;
								font-size: 1.2em;
								width: 100px;
								box-sizing: border-box;
							}
							
							.produto-adiciona-estoque-mais{
								border-top-right-radius: 5px;
								border-bottom-right-radius: 5px;
								background-color: #fdfdfd;
								border: none;
								height: 28px;
								box-sizing: border-box;
								padding: 0px 10px 0px 10px;

								&:hover{
									background-color: #999999;
									color: #ffffff;
								}

							}
						}

						.produto-adiciona-estoque-confirmar{
							border: none;
							background-color: #999999;
							color: #ffffff;
							font-size: 1.1em;
							margin: 20px 0px 0px 0px;
							padding: 5px 10px 5px 10px;
							width: 152px;
							box-sizing: border-box;
							border-radius: 5px;
							box-shadow: 1px 1px 2px 0px #444444;

							&:hover{
								background-color: #333333;
							}

						}
					}

					.produto-estoque-modal-escondido{
						display: none;
						visibility: hidden;
					}

					.produto-estoque-modal{
						position: absolute;
						top: 0;
						bottom: 0;
						left: 0;
						right: 0;
						width: 100vw;
						height: 100%;
						background-color: rgba(#999999,0.3);
						z-index: 1;
						display: flex;
						flex-direction: column;
						align-items: center;
						justify-content: start;

						.card{
							position: fixed;
							margin: 30px 0px 0px 0px;
							padding: 10px 10px 10px 10px;
							background-color: #f0f0f0;
							box-shadow: 1px 1px 2px 0px #999999;
							border-radius: 5px;
							display: flex;
							flex-direction: column;
							align-items: start;
							justify-content: start;

							.header{
								font-size: 1.2em;
								font-weight: bold;
								margin: 0px 0px 10px 0px;
							}

							.body{
								margin: 0px 0px 20px 0px;
							}

							.footer{
								
								display: flex;
								flex-direction: row;
								align-items: center;
								justify-content: center;

								.excluir-produto-confirmar{
									margin: 0px 10px 0px 0px;
									font-size: 1.2em;
									border: none;
									border-radius: 5px;
									background-color: #11bb33;
									color: #f0f0f0;
									box-shadow: 1px 1px 2px 0px #999999;
									padding: 5px 10px 5px 10px;

									&:hover{
										background-color: #119933;
									}
								}

								.excluir-produto-cancelar{
									font-size: 1.2em;
									border: none;
									border-radius: 5px;
									background-color: #ff5522;
									color: #f0f0f0;
									box-shadow: 1px 1px 2px 0px #999999;
									padding: 5px 10px 5px 10px;

									&:hover{
										background-color: #ff3300;
									}
								}
							}
						}
					}

				}

			}

			#lista-vazia{
				padding: 10px 10px 10px 10px;
				border-radius: 5px;
				background-color: #e0e0e0;
				box-shadow: 1px 1px 2px 0px #999999;
				width: 300px;
				align-self: center;
				text-align: center;
			}
		}

	}

